@using collection.Model

<table class="table">
    <thead>
        @TableHeader
    </thead>
    <tbody>
        @TableBody
    </tbody>
</table>

@code {
    [Parameter] public RenderFragment TableHeader { get; set; }
    [Parameter] public RenderFragment TableBody { get; set; }
    [Parameter] public Func<int, Task<IPagination?>> LoadPageFunction { get; set; }
}